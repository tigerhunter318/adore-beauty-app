{
  "name": "adore-beauty-native-app",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 14.0.0"
  },
  "scripts": {
    "start": "react-native start",
    "reset:cache": "react-native start --reset-cache",
    "clear:cache": "rm -fr $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && watchman watch-del-all",
    "clear:android": "rm -Rf ~/.gradle/caches android/.gradle android/build android/app/build",
    "clean:android": "echo 'use: 'cd android; ./gradlew clean''",
    "clear:ios": "rm -Rf ios/Pods ios/build ~/Library/Developer/Xcode/DerivedData",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "emulator:s8": "emulator -avd Galaxy_S8_API_29",
    "ios": "concurrently 'yarn start' 'yarn iphone:14'",
    "ios:ipad": "react-native run-ios --simulator=\"iPad mini (6th generation)\"",
    "ios:ipod": "react-native run-ios --simulator=\"iPod touch (7th generation)\"",
    "ios:12": "react-native run-ios --simulator=\"iPhone 12\"",
    "ios:small": "react-native run-ios --simulator=\"iPhone 13 mini\"",
    "ios:13": "react-native run-ios --simulator=\"iPhone 13\"",
    "iphone:14": "react-native run-ios --simulator=\"iPhone 14\"",
    "ios:old": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:release": "react-native run-ios --simulator=\"iPhone X\" --configuration=release",
    "ios:device": "react-native run-ios --device=\"iPhone 10\"",
    "ios:phil": "react-native run-ios --device=\"Philip’s iPhone\"",
    "ios:phil:release": "react-native run-ios --device=\"Philip’s iPhone\" --configuration=release",
    "ios:device:release": "react-native run-ios --device=\"iPhone\" --configuration=release",
    "jest:ci": "jest --verbose --ci",
    "test": "jest --watch",
    "test:silent": "jest --watchAll --silent",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watchAll",
    "test:debug1": "node --inspect-brk node_modules/.bin/jest --runInBand --watchAll",
    "e2e": "detox test --reuse --loglevel trace --configuration ios.sim.debug ",
    "detox:release": "detox build --configuration ios.sim.release && detox test --configuration ios.sim.release",
    "detox:build": "detox build --configuration ios.sim.debug",
    "detox:test": "detox test --configuration ios.sim.debug e2e/",
    "detox:clear": "rm -Rf ios/build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "webviews:build:addressfinder": "webpack --config webviews/webpack-addressfinder.js --mode production",
    "webviews:dev:addressfinder": "webpack-dev-server --hot --inline --config webviews/webpack-addressfinder.js",
    "webviews:build:afterpay": "webpack --config webviews/webpack-afterpay.js --mode production",
    "webviews:dev:afterpay": "webpack-dev-server --hot --inline --config webviews/webpack-afterpay.js",
    "webviews:build": "yarn webviews:build:afterpay && yarn webviews:build:addressfinder",
    "webviews:links": "cd webviews/universal-links; python3 -m http.server 3000",
    "upload-dsyms": "ios/Pods/FirebaseCrashlytics/upload-symbols -gsp ios/GoogleService-Info.plist -p ios ~/Downloads/appDsyms",
    "pods": "pod-install --quiet",
    "update-snapshots": "jest -u",
    "postinstall": "patch-package",
    "setup": "yarn && npx pod-install"
  },
  "jest": {
    "preset": "react-native",
    "verbose": false,
    "setupFiles": [
      "./jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "__mocks__"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native|@react-navigation|@expo|expo|@unimodules|@sentry|sentry-expo|tealium-react-native|search-insights)"
    ]
  },
